{
        "branches": [
          "master",
          "main",
          {
            "name": "beta",
            "prerelease": true
          },
          {
            "name": "alpha",
            "prerelease": true
          }
        ],
        "plugins": [
          ["@semantic-release/commit-analyzer", {
            "preset": "conventionalcommits",
            "releaseRules": [
              {
                "type": "major",
                "release": "major"
              },
              {
                "type": "minor",
                "release": "minor"
              },
              {
                "type": "bugfix",
                "release": "patch"
              }
              ]
          }],
          [
            "@semantic-release/release-notes-generator",
            {
              "preset": "conventionalcommits",
              "presetConfig": {
                "types": [
                  {
                    "type": "major",
                    "section": "‚ú® Major",
                    "hidden": false
                  },
                  {
                    "type": "bugfix",
                    "section": "üêõ Bug-Fix",
                    "hidden": false
                  },
                  {
                    "type": "minor",
                    "section": "üìù Minor Changes",
                    "hidden": false
                  }
                ]
              }
            }
          ],

          [
            "@semantic-release/changelog"
          ],
          [
            "@semantic-release/github"
          ],
          [
            "@semantic-release/git"
          ]
        ]
}
  