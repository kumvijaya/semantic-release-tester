{
    "name": "semantic-release-tester",
    "repository": {
      "type": "git",
      "url": "https://github.com/kumvijaya/semantic-release-tester.git"
    },
    "private": true,
    "release": {
        "branches": [
          "master",
          "main",
          {
            "name": "beta",
            "prerelease": true
          },
          {
            "name": "alpha",
            "prerelease": true
          }
        ],
        "plugins": [
          ["@semantic-release/commit-analyzer", {
            "preset": "conventionalcommits",
            "releaseRules": [
              {
                  "type": "major",
                  "release": "major"
              },
              {
                  "message": "*([)minor(])*",
                  "release": "minor"
              },
              {
                  "message": "*([)bug-fix(])*",
                  "release": "patch"
              }
              ]
          }],
          [
            "@semantic-release/release-notes-generator",
            {
              "preset": "conventionalcommits",
              "presetConfig": {
                "types": [
                  {
                    "type": "major",
                    "section": "‚ú® Major",
                    "hidden": false
                  },
                  {
                    "message": "*([)bug-fix(])*",
                    "section": "üêõ Bug-Fix",
                    "hidden": false
                  },
                  {
                    "message": "*([)minor(])*",
                    "section": "üìù Minor Changes",
                    "hidden": false
                  }
                ]
              }
            }
          ],

          [
            "@semantic-release/changelog"
          ],
          [
            "@semantic-release/github"
          ],
          [
            "@semantic-release/git"
          ]
        ]
      }
  }